{
  "suggest.noselect":false,
  "suggest.preferCompleteThanJumpPlaceholder":true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"],
      "initializationOptions": {
        "diagnosticsEnabled": true,
        "formatTool": "goimports",
        "lintTool": "golint"
      }
    }
  }
}
